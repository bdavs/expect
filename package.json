{
	"name": "expect",
	"displayName": "Expect",
	"description": "Expect Language Support",
	"version": "0.1.0",
	"publisher": "bdavs",
	"repository": {
        "type": "git",
        "url": "https://github.com/bdavs/expect"
    },
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "expect",
				"aliases": [
					"Expect",
					"expect"
				],
				"extensions": [
                    ".exp",
                    ".expect"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "expect",
				"scopeName": "source.exp",
				"path": "./syntaxes/expect.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "expect",
				"path": "./snippets/expect_snippets.json"
			}
		]
    },
    "license": "MIT",
